{
  "manifest_version": 2,

  "name": "Tiler",
  "description": "This extension launches quick access to tiler",
  "version": "1.0",

  "browser_action": {
    "default_icon": "img/Icons/logo.png",
    "default_popup": "ChromeExtension.html"
  },
  "background": {
    "scripts": ["Scripts/jquery-2.1.0.min.js","Scripts/WebControl.js", "Scripts/HomePageControl.js", "Scripts/extensionBackground.js"]
  },
  "permissions": [
    "activeTab",
    "alarms"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://*.jquery.com https://localhost:44305; object-src 'self'"
}