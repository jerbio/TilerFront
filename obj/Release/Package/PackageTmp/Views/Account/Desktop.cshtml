

<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Tiler Desktop</title>
    @Scripts.Render("~/Content/TilerDesktopJS")
</head>

<body>
    <div id="Header">
        <span class="TilerLogo"></span>
        <div id="AddEventButton" class="ControlPanelButton"></div>
    </div>
    <div id="SearchBarAndContentContainer"><input id="SearchBar"><span class="ControlPanelButton" id="ControlPanelSearchButton"></span></div>
    <div id="MonthBar"></div>
    <div id="AddEventPanel"></div>
    <div id="FullWeekContainer"> </div>
    <div id="TimeBar">
        <ul>
            <li>12 AM</li>
            <li>1</li>
            <li>2</li>
            <li>3</li>
            <li>4</li>
            <li>5</li>
            <li>6</li>
            <li>7</li>
            <li>8</li>
            <li>9</li>
            <li>10</li>
            <li>11</li>
            <li>12 PM</li>
            <li>1</li>
            <li>2</li>
            <li>3</li>
            <li>4</li>
            <li>5</li>
            <li>6</li>
            <li>7</li>
            <li>8</li>
            <li>9</li>
            <li>10</li>
            <li>11</li>
        </ul>
    </div>
    <div id="LeftPane">
        <div id="MonthGrid"> </div>
    </div>




    <div id="ControlPanelContainer" tabindex="1">
        <div id="InfoContainer">
            <div id="ControlPanelNameOfSubevent">
                <div class="InfoControlPanel" id="ControlPanelNameOfSubeventInfo"></div>
            </div>
            <div id="ControlPanelSubEventTime">
                <div class="InfoControlPanel" id="ControlPanelSubEventTimeInfo"></div>
            </div>
            <div id="ControlPanelDeadlineOfSubevent">
                Deadline:
                <div class="InfoControlPanel" id="ControlPanelDeadlineOfSubeventInfo"></div>
            </div>
        </div>
        <div id="InputContainer">
            <div class="ControlPanelButton" id="ControlPanelLocationButton"></div>
            <div class="ControlPanelButton" id="ControlPanelProcrastinateButton"></div>
            <div class="ControlPanelButton" id="ControlPanelDeleteButton"></div>
            <div class="ControlPanelButton" id="ControlPanelCompleteButton"></div>
            <div class="ControlPanelButton" id="ControlPanelCloseButton"></div>
        </div>
        <div id="ProcrastinateEventModal" tabindex="1">
            Procrastinate this by
            <div>
                <span>H</span>
                <input id="procrastinateHours" type="number">
                <span>M</span>
                <input id="procrastinateMins" type="number">
                <span>D</span>
                <input id="procrastinateDays" type="number">
                <input id="submitProcatination" type="submit">
            </div>
        </div>
        <div id="ConfirmDeleteModal">
            <div id="ConfirmContent">
                <span id="DeleteMessage">Sure you want to delete this?</span>
                <button class="SubmitButton" id="YeaToConfirmDelete">Yea</button>
                <button class="SubmitButton" id="NayToConfirmDelete">Nay</button>
            </div>
        </div>
        <div class='hideMultiSelectPanel' id="MultiSelectPanel"></div>
    </div>
    <div id="LoadingScreenPanel"></div>
</body>
</html>






